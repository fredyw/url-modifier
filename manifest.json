{
  "manifest_version": 2,
  "name": "url-modifier",
  "version": "0.1.0",
  "description": "This extension allows modifying URL automatically",
  "homepage_url": "https://github.com/fredyw/url-modifier",
  "background": {
    "scripts": ["urlmodifier.js"]
  },
  "browser_action": {
  },
//  "content_scripts": [
//    {
//      "matches": ["<all_urls>"],
//      "js": ["urlmodifier.js"],
//      "run_at": "document_start"
//    }
//  ],
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
